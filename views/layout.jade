doctype html
html
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name="keywords" content="Minneapolis, voting, votes, map, city council, elections, candidates")
    meta(name="robots" content="index,nofollow")
    meta(name="language" content="English")

    meta(property="og:type" content="article")
    meta(property="og:title" content="Election Results Map for the City of Minneapolis")
    meta(property="og:site_name" content="Election Results Map Minneapolis")
    meta(property="og:url" content="http://www.mplselectionmap.com")
    meta(property="og:image" content="/images/screenshot-for-og.png")
    meta(property="article:published_time" content="2017-03-31")
    meta(property="article:author" content="https://www.facebook.com/matt.thrasher2")
    meta(name="twitter:card" content ="app")
    meta(name="twitter:site" content ="@mthrasher33")
    meta(name="twitter:description" content ="Explore election election results for the City of Minneapolis on our map.")
    meta(name="twitter:app:name:iphone" content ="Election Results Map Minneapolis")
    meta(name="twitter:app:name:ipad" content ="Election Results Map Minneapolis")
    meta(name="twitter:app:name:googleplay" content ="Election Results Map Minneapolis")


    title #{title} - Minneapolis Election Results
    link(rel='stylesheet',href='/stylesheets/bootstrap.min.css')
    link(rel='stylesheet', href='/stylesheets/dataTables.bootstrap.min.css')
    link(rel='stylesheet',href='/stylesheets/global.css')

  body
    .body-content
        div.container
            div.row.map-holder
                div.col-xs-12
                        h1.map-title #{pageTitle} 
                            span.author made by 
                                a(href='https://twitter.com/mthrasher33') Matt Thrasher
                            span.author | 
                                a(href='https://elections.wedgelive.com/') Get Involved 2017
                block content


